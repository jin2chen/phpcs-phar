{
  "name": "jin2chen/phpcs-phar",
  "description": "PHP_CodeSniffer Phar distribution.",
  "type": "library",
  "keywords": [
    "phpcs",
    "standards"
  ],
  "homepage": "https://github.com/squizlabs/PHP_CodeSniffer",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Greg Sherwood",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/squizlabs/PHP_CodeSniffer/issues",
    "wiki": "https://github.com/squizlabs/PHP_CodeSniffer/wiki",
    "source": "https://github.com/squizlabs/PHP_CodeSniffer"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  },
  "require": {
    "php": ">=5.4.0",
    "ext-tokenizer": "*",
    "ext-xmlwriter": "*",
    "ext-simplexml": "*"
  },
  "conflict": {
    "squizlabs/php_codesniffer": "*"
  },
  "bin": [
    "phpcs",
    "phpcs.bat",
    "phpcs.phar",
    "phpcbf",
    "phpcbf.bat",
    "phpcbf.phar"
